#include <bits/stdc++.h>
#define mod 1000000007
#define ll long long
using namespace std;
#include <bits/stdc++.h>
using namespace std;
 
 
 
int main()
{
    int t;
    cin>>t;
    
    while(t--){
       vector<int> a(4);
       for(int i=0;i<4;i++){
        cin>>a[i];
       }
 
       int max_position = max_element(a.begin(),a.end()) - a.begin();
       int min_position = min_element(a.begin(),a.end()) - a.begin();
 
       int sum = max_position + min_position;
 
       if(sum == 3) cout<<"YES";
       else cout<<"NO";
 
       cout<<endl;
       
       
    }
    return 0;
}
